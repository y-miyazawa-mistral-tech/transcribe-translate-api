{
    "version": 1,
    "formatters": {
        "default": {
            "format": "%(asctime)s - %(levelname)s - %(name)s - %(module)s - %(funcName)s() L%(lineno)d: %(message)s"
        }
    },
    "handlers": {
        "console": {
            "formatter": "default",
            "class": "logging.StreamHandler",
            "level": "DEBUG"
        }
    },
    "root": {
        "handlers": [
            "console"
        ],
        "level": "DEBUG"
    }
}